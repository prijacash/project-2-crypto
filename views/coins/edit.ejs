<h2>EDIT FORM </h2>

<form method="POST", action="/coins/<%= coin.id %>/?_method=PUT">
    <label>Name</label>
    <input type="text" name="name" value="<%= coin.name %>">

    <label>Description</label>
    <input type="text" name="description" value="<%= coin.description %>">

    <input type="submit">
</form>