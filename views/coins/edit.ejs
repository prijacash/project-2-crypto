<h2>EDIT FORM </h2>

<form method="POST" action="/coins/edit/<%= myCoin.id %>/?_method=PUT">

    <label>Name</label>
    <input type="text" name="name" value="<%= myCoin.name %>">

    <label>Description</label>
    <input type="text" name="description" value="<%= myCoin.description %>">

    <button type="submit">Update</button>
</form>